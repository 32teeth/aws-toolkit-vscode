{
    "version": "2.0",
    "metadata": {
        "apiVersion": "2018-05-10",
        "endpointPrefix": "weaverbirdservice",
        "jsonVersion": "1.1",
        "protocol": "rest-json",
        "serviceFullName": "A beautiful and amazing WeaverbirdService",
        "serviceId": "Weaverbird",
        "signatureVersion": "v4",
        "signingName": "execute-api",
        "uid": "weaverbird-2018-05-10"
    },
    "operations": {
        "Echo": {
            "name": "Echo",
            "http": {
                "method": "POST",
                "requestUri": "/echo",
                "responseCode": 200
            },
            "input": { "shape": "EchoInput" },
            "output": { "shape": "EchoOutput" },
            "errors": [{ "shape": "InvalidInputException" }]
        },
        "GenerateApproach": {
            "name": "GenerateApproach",
            "http": {
                "method": "POST",
                "requestUri": "/generateApproach",
                "responseCode": 200
            },
            "input": { "shape": "GenerateApproachInput" },
            "output": { "shape": "GenerateApproachOutput" },
            "errors": [{ "shape": "InvalidInputException" }]
        },
        "GenerateCode": {
            "name": "GenerateCode",
            "http": {
                "method": "POST",
                "requestUri": "/generateCode",
                "responseCode": 200
            },
            "input": { "shape": "GenerateCodeInput" },
            "output": { "shape": "GenerateCodeOutput" },
            "errors": [{ "shape": "InvalidInputException" }]
        }
    },
    "shapes": {
        "DeletedFilesList": {
            "type": "list",
            "member": { "shape": "String" }
        },
        "EchoInput": {
            "type": "structure",
            "members": {
                "string": { "shape": "String" }
            }
        },
        "EchoOutput": {
            "type": "structure",
            "members": {
                "string": { "shape": "String" }
            }
        },
        "FileMetadata": {
            "type": "structure",
            "required": ["filePath", "fileContent"],
            "members": {
                "filePath": { "shape": "String" },
                "fileContent": { "shape": "String" }
            }
        },
        "FileMetadataList": {
            "type": "list",
            "member": { "shape": "FileMetadata" }
        },
        "GenerateApproachInput": {
            "type": "structure",
            "required": ["originalFileContents", "task"],
            "members": {
                "originalFileContents": { "shape": "FileMetadataList" },
                "task": { "shape": "String" }
            }
        },
        "GenerateApproachOutput": {
            "type": "structure",
            "members": {
                "approach": { "shape": "String" },
                "background": { "shape": "String" }
            }
        },
        "GenerateCodeInput": {
            "type": "structure",
            "required": ["originalFileContents", "task", "approach"],
            "members": {
                "originalFileContents": { "shape": "FileMetadataList" },
                "task": { "shape": "String" },
                "approach": { "shape": "String" }
            }
        },
        "GenerateCodeOutput": {
            "type": "structure",
            "members": {
                "newFileContents": { "shape": "FileMetadataList" },
                "deletedFiles": { "shape": "DeletedFilesList" }
            }
        },
        "InvalidInputException": {
            "type": "structure",
            "members": {
                "message": { "shape": "String" }
            },
            "error": {
                "httpStatusCode": 400,
                "senderFault": true
            },
            "exception": true
        },
        "String": { "type": "string" }
    }
}
