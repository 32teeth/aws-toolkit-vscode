{
    "types": [
        {
            "name": "architecture",
            "type": "string",
            "description": "SAM Application architecture",
            "allowedValues": ["x86_64", "arm64"]
        },
        {
            "name": "consolasAcceptedTokens",
            "type": "int",
            "description": "The metrics accepted on suggested consolas code"
        },
        {
            "name": "consolasAutomatedtriggerType",
            "type": "string",
            "description": "The type of the Automated trigger to send request to Consolas service",
            "allowedValues": ["KeyStrokeCount", "SpecialCharacters", "Enter", "IntelliSenseAcceptance"]
        },

        {
            "name": "consolasCodeScanJobId",
            "type": "string",
            "description": "The ID of the security scan job"
        },
        {
            "name": "consolasCodeScanLines",
            "type": "int",
            "description": "How many lines of code being sent for security scan"
        },
        {
            "name": "consolasCodeScanPayloadSize",
            "type": "int",
            "description": "The payload size of customer project context sent for security scan"
        },
        {
            "name": "consolasCodeScanTotalIssues",
            "type": "int",
            "description": "The number of security issues been detected"
        },
        {
            "name": "consolasCompletionType",
            "type": "string",
            "description": "Completion Type of the inference results returned from Consolas model layer",
            "allowedValues": ["Line", "Block"]
        },
        {
            "name": "consolasCursorOffset",
            "type": "int",
            "description": "cursor location offset in the editor when invoking Consolas for recommendation"
        },
        {
            "name": "consolasLanguage",
            "type": "string",
            "description": "Programming language of the Consolas recommendation",
            "allowedValues": ["java", "python", "javascript", "plaintext"]
        },
        {
            "name": "consolasLineNumber",
            "type": "int",
            "description": "The line number of the cursor when the event happens"
        },
        {
            "name": "consolasModificationPercentage",
            "type": "double",
            "description": "The percentage of user modifications on the suggested code"
        },
        {
            "name": "consolasPaginationProgress",
            "type": "int",
            "description": "The number of recommendations received so far when user makes a decision."
        },
        {
            "name": "consolasPercentage",
            "type": "int",
            "description": "The percentage of acceptance on suggested consolas code on the overall code"
        },
        {
            "name": "consolasRequestId",
            "type": "string",
            "description": "The ID of the request to Consolas service"
        },
        {
            "name": "consolasRuntime",
            "type": "string",
            "allowedValues": ["java8", "java11", "java16", "python2", "python3", "javascript", "unknown"],
            "description": "the pre-defined set of values for runtime version of the language of Consolas recommendation"
        },
        {
            "name": "consolasRuntimeSource",
            "type": "string",
            "description": "the original (free-text) of the runtime version of the language of Consolas recomendation"
        },
        {
            "name": "consolasSessionId",
            "type": "string",
            "description": "The ID of the session to Consolas service"
        },
        {
            "name": "consolasStartTime",
            "type": "string",
            "description": "The start time of user typed code in minutes"
        },
        {
            "name": "consolasSuggestionIndex",
            "type": "int",
            "description": "The index for each suggestion, respectively, in the list of suggestions returned from service invocation."
        },
        {
            "name": "consolasLastSuggestionIndex",
            "type": "int",
            "description": "The last index of recommendation from a particular response."
        },
        {
            "name": "consolasSuggestionReferences",
            "type": "string",
            "description": "The references of the suggestion."
        },
        {
            "name": "consolasSuggestionState",
            "type": "string",
            "description": "User decision of each of the suggestion returned from Consolas",
            "allowedValues": ["Accept", "Reject", "Discard", "Ignore", "Filter", "Unseen"]
        },
        {
            "name": "consolasTotalTokens",
            "type": "int",
            "description": "The metrics generated by the user and acceptance of suggested consolas code in the language consolas supports."
        },
        {
            "name": "consolasTriggerType",
            "type": "string",
            "description": "The type of the user trigger to send request to Consolas service",
            "allowedValues": ["OnDemand", "AutoTrigger"]
        },
        {
            "name": "documentFormat",
            "type": "string",
            "allowedValues": ["JSON, YAML"],
            "description": "SSM Create document format selection"
        },
        {
            "name": "ssmOperation",
            "type": "string",
            "allowedValues": ["Create", "Update"],
            "description": "SSM Publish Document operation type"
        },
        {
            "name": "starterTemplate",
            "type": "string",
            "description": "Starter template chosen during create document"
        }
    ],
    "metrics": [
